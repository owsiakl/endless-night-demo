{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "target": "es2022",
    "module": "commonjs",
    "typeRoots": [
      "src/Loader/GLTF/@types",
      "src/@types"
    ],
    "sourceMap": true,
    "outDir": "public/dir",
    "removeComments": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
